@{
    Layout = "../Shared/_Inner.cshtml";
    var turl = ViewBag.Template;
}

<div class="col-lg-12" style="width: 540px; margin: 5px;">
    <form method="post" class="form-inline" id="eform" enctype="multipart/form-data">
        <div class="form-group">
            <input type="file" id="efile" name="efile" class="form-control" style="width: 300px; display: inline;"/>
            @if (!string.IsNullOrEmpty(turl))
            {
                <a href="@turl">下载模版</a>
            }
        </div>
    </form>
    <table class="table table-bordered" style="margin-top: 5px; font-weight: 600;">
        <tbody id="ebody"></tbody>
    </table>
</div>
